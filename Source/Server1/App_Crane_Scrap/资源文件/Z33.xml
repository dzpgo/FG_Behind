<WorkInstructionsConfig>

<!-- 起卷作业步骤定义-->
     <!-- target 是单一的 如果没有任何指定，那么用描述符ANY  -->	
     <!-- currentAreaList是可以多个的，如果没有任何指定，那么用描述符ANY 该项目不能为空 -->		 
	 <!-- CurrentAreaExceptionList 可以是多个的，如果没有例外，就为空，有例外，就一项一项明确，该项目上不能使用ANY描述符 -->
	 
	<!-- 在1SACL 2SACL起卷，当前区域在SACL和黄点区域以外的所有区域-->
	<WorkInstruction>
		<Name>LIFT_COIL_FROM_SACL_ENTRY_1</Name>
		<TYPE>COIL_UP</TYPE>
		<Target>SACL_ENTRY</Target>
		<CurrentAreaList>ANY</CurrentAreaList>
		<CurrentAreaExceptionList>SACL_ENTRY,SACL_LANE_3</CurrentAreaExceptionList>
		<CanAcceptEvade>1</CanAcceptEvade>
		<Action>MOVE_TO_SPECIAL_POINT</Action>
		
		<SpecialPoint> <X>7500</X> <Y>13500</Y> </SpecialPoint>		
	</WorkInstruction>
	
	<!-- 在1SACL 2SACL起卷，当前区域在SACL入口和和黄点区域以内-->
	<WorkInstruction>
		<Name>LIFT_COIL_FROM_SACL_ENTRY_2</Name>
		<TYPE>COIL_UP</TYPE>	
		<Target>SACL_ENTRY</Target>
		<CurrentAreaList>SACL_ENTRY,SACL_LANE_3</CurrentAreaList>
		<CurrentAreaExceptionList></CurrentAreaExceptionList>
		<CanAcceptEvade>0</CanAcceptEvade>		
		<Action>CHECK_COIL_POS</Action>	
		<Action>LOCK_SADDLE</Action>				
		<Action>MOVE_TO_COIL_POS</Action>		
		<Action>COIL_UP</Action>				
		<Action>UNLOCK_SADDLE</Action>		
		<Action>MOVE_TO_SPECIAL_POINT</Action>		
		<Action>FINISH_JOB</Action>		
		
		<SpecialPoint> <X>7500</X> <Y>13500</Y> </SpecialPoint>		
	</WorkInstruction>	
	
	<!-- 在D202起卷，当前区域在D202区域以外的任何的区域-->	
	<WorkInstruction>
		<Name>LIFT_COIL_FROM_D202_1 </Name>
		<TYPE>COIL_UP</TYPE>	
		<Target>D202_EXIT</Target>
		<CurrentAreaList>ANY</CurrentAreaList>
		<CurrentAreaExceptionList>SACL_ENTRY,D202_EXIT</CurrentAreaExceptionList>
		<CanAcceptEvade>1</CanAcceptEvade>
		<Action>CHECK_COIL_POS</Action>				
		<Action>MOVE_TO_COIL_POS</Action>			
	</WorkInstruction>	
	
	<!-- 在D202起卷，当前区域在D202区域-->		
	<WorkInstruction>
		<Name>LIFT_COIL_FROM_D202_2 </Name>
		<TYPE>COIL_UP</TYPE>		
		<Target>D202_EXIT</Target>
		<CurrentAreaList>D202_EXIT</CurrentAreaList>
		<CurrentAreaExceptionList></CurrentAreaExceptionList>
		<CanAcceptEvade>0</CanAcceptEvade>
		<Action>CHECK_COIL_POS</Action>	
		<Action>LOCK_SADDLE</Action>				
		<Action>MOVE_TO_COIL_POS</Action>		
		<Action>COIL_UP</Action>				
		<Action>UNLOCK_SADDLE</Action>		
		<Action>FINISH_JOB</Action>			
	</WorkInstruction>	
	
	<!-- 在一般库区内起卷当前位置不包括1# 2#SCAL入口区域-->	
	<WorkInstruction>
		<Name>DEFAULT_UP</Name>
		<TYPE>COIL_UP</TYPE>		
		<Target>ANY</Target>
		<CurrentAreaList>ANY</CurrentAreaList>
		<CurrentAreaExceptionList>SACL_ENTRY</CurrentAreaExceptionList>
		<CanAcceptEvade>1</CanAcceptEvade>
		<Action>CHECK_COIL_POS</Action>	
		<Action>LOCK_SADDLE</Action>				
